{
  "name": "dicewerx",
  "version": "1.0.10",
  "description": "Standard dice notation parser and evaluator",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint --project \"./tsconfig.json\"",
    "docs": "typedoc --out docs src",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs && echo $null >> ./docs/.nojekyll",
    "prep": "npm run lint && npm run test && npm run build && npm run gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemastermick/dicewerx.git"
  },
  "author": "codemastermick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codemastermick/dicewerx/issues"
  },
  "homepage": "https://github.com/codemastermick/dicewerx#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "gh-pages": "^2.1.0",
    "mocha": "^6.2.0",
    "tslint": "^5.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "files": [
    "dist"
  ],
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {},
  "keywords": [
    "dice",
    "rolling",
    "rpg",
    "d20"
  ]
}
